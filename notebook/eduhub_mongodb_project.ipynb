   "source": [
    "##### Course Schema and Collection"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "198f0deb",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      " 'courses' collection created with validation rules!\n"
     ]
    }
   ],
   "source": [
    "from pymongo import MongoClient\n",
    "\n",
    "# 1. Connect to MongoDB\n",
    "client = MongoClient("mongodb://127.0.0.1:27017/"),
    "db = client[\"eduhub_db\"]\n",
    "\n",
    "# 2. Define JSON Schema validation for \"courses\" collection\n",
    "course_schema = {\n",
    "    \"$jsonSchema\": {\n",
    "        \"bsonType\": \"object\",\n",
    "        \"required\": [\"courseId\", \"title\", \"instructorId\", \"level\", \"duration\", \"price\", \"createdAt\", \"isPublished\"],\n",
    "        \"properties\": {\n",
    "            \"_id\": {\n",
    "                \"bsonType\": \"objectId\",\n",
    "                \"description\": \"auto-generated ObjectId\"\n",
    "            },\n",
    "            \"courseId\": {\n",
    "                \"bsonType\": \"string\",\n",
    "                \"description\": \"must be a unique string identifier for the course\"\n",
    "            },\n",
    "            \"title\": {\n",
    "                \"bsonType\": \"string\",\n",
    "                \"description\": \"course title is required\"\n",
    "            },\n",
    "            \"description\": {\n",
    "                \"bsonType\": \"string\",\n",
    "                \"description\": \"optional course description\"\n",
    "            },\n",
    "            \"instructorId\": {\n",
    "                \"bsonType\": \"string\",\n",
    "                \"description\": \"must be a string referencing a userId in users collection\"\n",
    "            },\n",
    "            \"category\": {\n",
    "                \"bsonType\": \"string\",\n",
    "                \"description\": \"course category (e.g., Data Science, AI)\"\n",
    "            },\n",
    "            \"level\": {\n",
    "                \"enum\": [\"beginner\", \"intermediate\", \"advanced\"],\n",
    "                \"description\": \"course level must be one of 'beginner', 'intermediate', or 'advanced'\"\n",
    "            },\n",
    "            \"duration\": {\n",
    "                \"bsonType\": [\"int\", \"double\"],\n",
    "                \"minimum\": 0,\n",
    "                \"description\": \"course duration in hours\"\n",
    "            },\n",
    "            \"price\": {\n",
    "                \"bsonType\": [\"int\", \"double\"],\n",
    "                \"minimum\": 0,\n",
    "                \"description\": \"course price in numeric format\"\n",
    "            },\n",
    "            \"tags\": {\n",
    "                \"bsonType\": \"array\",\n",
    "                \"items\": {\n",
    "                    \"bsonType\": \"string\"\n",
    "                },\n",
    "                \"description\": \"array of tags (optional)\"\n",
    "            },\n",
    "            \"createdAt\": {\n",
    "                \"bsonType\": \"date\",\n",
    "                \"description\": \"date the course was created\"\n",
    "            },\n",
    "            \"updatedAt\": {\n",
    "                \"bsonType\": \"date\",\n",
    "                \"description\": \"date the course was last updated\"\n",
    "            },\n",
    "            \"isPublished\": {\n",
    "                \"bsonType\": \"bool\",\n",
    "                \"description\": \"whether the course is published\"\n",
    "            }\n",
    "        }\n",
    "    }\n",
    "}\n",
    "\n",
    "print(\" 'courses' collection created with validation rules!\")\n"
