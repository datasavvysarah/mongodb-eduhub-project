    "            \"total_submissions\": {\"$size\": \"$submissions\"},\n",
    "            \"average_grade\": {\"$avg\": \"$submissions.grade\"}\n",
    "        }\n",
    "    }\n",
    "])\n",
    "\n",
    "# Convert aggregation result to a list\n",
    "engagement_data = list(engagement_metrics)\n",
    "\n",
    "# Prepare data for plotting\n",
    "user_ids = [entry[\"userId\"] for entry in engagement_data]\n",
    "total_submissions = [entry[\"total_submissions\"] for entry in engagement_data]\n",
    "average_grades = [entry[\"average_grade\"] if entry[\"average_grade\"] is not None else 0 for entry in engagement_data]\n",
    "\n",
    "# Plotting\n",
    "plt.figure(figsize=(12, 6))\n",
