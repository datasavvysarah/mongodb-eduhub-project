    }
   ],
   "source": [
    "category_popularity = db.enrollments.aggregate([\n",
    "    {\n",
    "        \"$lookup\": {\n",
    "            \"from\": \"courses\",\n",
    "            \"localField\": \"courseId\",\n",
    "            \"foreignField\": \"courseId\",\n",
    "            \"as\": \"course_info\"\n",
    "        }\n",
    "    },\n",
    "    {   \"$unwind\": \"$course_info\" },\n",
    "    {\n",
    "        \"$group\": {\n",
    "            \"_id\": \"$course_info.category\",   # group by course category\n",
    "            \"total_enrollments\": {\"$sum\": 1}   # count enrollments\n",
    "        }\n",
    "    },\n",
    "    {\n",
    "        \"$sort\": {\"total_enrollments\": -1}   # sort by total enrollments descending\n",
    "    }\n",
    "])\n",
    "\n",
    "# Convert aggregation result to a list\n",
    "category_data = list(category_popularity)\n",
    "# Prepare data for plotting\n",
    "categories = [entry[\"_id\"] for entry in category_data]\n",
    "enrollments = [entry[\"total_enrollments\"] for entry in category_data]\n",
    "\n",
    "# Plotting\n",
    "plt.figure(figsize=(10, 6)) \n",
    "plt.bar(categories, enrollments, color='green')\n",
    "plt.title(\"Most Popular Course Categories\")\n",
    "plt.xlabel(\"Category\")\n",
    "plt.ylabel(\"Total Enrollments\")\n",
    "plt.xticks(rotation=45)\n",
    "plt.grid(axis='y')\n",
    "plt.show()"
   ]
  },
