     "text": [
      "Course Completion Rates:\n",
      "{'_id': 'C004', 'completion_rate': 100.0}\n",
      "{'_id': 'C001', 'completion_rate': 100.0}\n",
      "{'_id': 'C008', 'completion_rate': 100.0}\n",
