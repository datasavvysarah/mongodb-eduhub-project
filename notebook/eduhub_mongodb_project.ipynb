      "{'_id': 'C003', 'completion_rate': 100.0}\n",
      "{'_id': 'C006', 'completion_rate': 100.0}\n"
     ]
    }
   ],
   "source": [
    "completion_rates = db.enrollments.aggregate([\n",
    "    {   \"$lookup\": {\n",
    "            \"from\": \"assignment_submissions\",\n",
    "            \"localField\": \"userId\",\n",
    "            \"foreignField\": \"userId\",\n",
    "            \"as\": \"submissions\"\n",
    "        }\n",
    "    },\n",
    "    {   \"$unwind\": \"$submissions\" },\n",
    "    {   \"$group\": {\n",
    "            \"_id\": \"$courseId\",\n",
    "            \"total_enrollments\": {\"$sum\": 1},\n",
    "            \"completed_assignments\": {\"$sum\": {\"$cond\": [{\"$gte\": [\"$submissions.grade\", 50]}, 1, 0]}}\n",
    "        }\n",
    "    },\n",
    "    {   \"$project\": {\n",
    "            \"completion_rate\": {\"$multiply\": [{\"$divide\": [\"$completed_assignments\", \"$total_enrollments\"]}, 100]}\n",
    "        }\n",
    "    }\n",
    "])\n",
    "print(\"Course Completion Rates:\")\n",
    "for rate in completion_rates:\n",
    "    print(rate)"
   ]
