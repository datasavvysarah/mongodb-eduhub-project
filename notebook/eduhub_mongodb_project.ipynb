   "source": [
    "##### Monthly enrollment trends\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 51,
   "id": "f9568b18",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1000x600 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "# Aggregation to get monthly enrollment counts\n",
    "monthly_enrollments = db.enrollments.aggregate([\n",
    "    {\n",
    "        \"$group\": {\n",
    "            \"_id\": {\n",
    "                \"year\": {\"$year\": \"$enrolledAt\"},\n",
    "                \"month\": {\"$month\": \"$enrolledAt\"}\n",
    "            },\n",
    "            \"total_enrollments\": {\"$sum\": 1}\n",
    "        }\n",
    "    },\n",
    "    {\n",
    "        \"$sort\": {\n",
    "            \"_id.year\": 1,\n",
    "            \"_id.month\": 1\n",
    "        }\n",
    "    }\n",
    "])\n",
    "\n",
    "# Convert aggregation result to a list\n",
    "enrollment_data = list(monthly_enrollments)\n",
    "# Prepare data for plotting\n",
    "dates = []\n",
    "enrollments = []\n",
    "for entry in enrollment_data:\n",
    "    year = entry[\"_id\"][\"year\"]\n",
    "    month = entry[\"_id\"][\"month\"]\n",
    "    total = entry[\"total_enrollments\"]\n",
    "    dates.append(datetime(year, month, 1))\n",
    "    enrollments.append(total)\n",
    "\n",
    "# Plotting\n",
    "plt.figure(figsize=(10, 6))\n",
    "plt.plot(dates, enrollments, marker='o')\n",
    "plt.title(\"Monthly Enrollment Trends\")\n",
    "plt.xlabel(\"Month\")\n",
    "plt.ylabel(\"Total Enrollments\")\n",
    "plt.grid(True)\n",
    "plt.gca().xaxis.set_major_formatter(mdates.DateFormatter('%Y-%m'))\n",
    "plt.gca().xaxis.set_major_locator(mdates.MonthLocator())\n",
    "plt.gcf().autofmt_xdate()\n",
    "plt.show()"
   ]
  },
